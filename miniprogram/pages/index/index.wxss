page{
  font-family: "微软雅黑";
  background-color:#eeeeee
}
/*头部搜索begin*/
.search {
    width: 100%;
    height: 92rpx;
    position: fixed;
    z-index: 9;
    background: rgba(0,0,0,.2);
}

.search-inner {
    width: 92%;
    height: 64rpx;
    margin-top: 11rpx;
    margin-left:5%;
    position: relative;
}
.search-l {
    width: 6%;
    font-size: 20rpx;
    color: white;
    margin-top:17rpx;
}
.search-l text {
    width: 40rpx;
    line-height: 24rpx;
    display: block;
}
.search-r {
    width: 90%;
    height: 64rpx;
    background-color: #f8f8f8;
    border-radius: 40rpx;
    margin-left:60rpx;
    margin-top:-65rpx;   
}
.search-img{
  position:absolute;
  top:11rpx;left:78rpx;
}
.search-r input {    
    font-size: 28rpx;
    /* text-indent: 65rpx; */
    padding-top:8rpx;
    padding-left:66rpx;
}
/*头部搜索end*/
/* 轮播图 */
.slide{
   width:100%; 
  height:210px; 
   background:rgba(255,255,255,.5);
}
.slide-image{
  width:100%; 
  height:210px; 
}
swiper-item{
  width:100%; 
  height:210px;
}
/* 图标 */
 .a{
  float:left;
  margin-left:8.45%;  
  margin-top:3%;
  width:15%;
} 
.a3{
  margin-top:20px;
  height:205px;
  width:100%;
  background:rgba(255,255,255,.5);
}
.a1{
  width:50px;
  height:50px;
  margin:5px;
}
.a2{
  font-size:13px;
}
.b{
  width:100%;
